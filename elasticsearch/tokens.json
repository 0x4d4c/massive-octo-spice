{
	"template": "cif.tokens-*",
	"tokens": {
		"properties": {
			"@version": {
				"index": "not_analyzed",
				"type": "string"
			},
			"@timestamp": {
				"type": "date",
				"format": "date_time"
			},
			"username": {
				"type": "string"
			},
			"groups": {
				"type": "string"
			},
			"token": {
				"type": "string"
			},
			"revoked": {
				"type": "boolean"
			},
			"read": {
				"type": "boolean"
			},
			"write": {
				"type": "boolean"
			},
			"acl": {
				"type": "string"
			},
			"expires": {
				"type": "date",
				"format": "date_time"
			}
		}
	}
}